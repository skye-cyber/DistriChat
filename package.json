{
    "name": "districhat",
    "version": "1.0",
    "private": true,
    "description": "Distributed system  application.",
    "scripts": {
        "start:node1": "python node1/manage.py runserver 0.0.0.0:8002",
        "start:node2": "python node2/manage.py runserver 0.0.0.0:8003",
        "node1": "python node1/manage.py",
        "node2": "python node2/manage.py",
        "start:nodes": "python nodes_initializer.py runserver",
        "start:main": "python central_server/manage.py runserver 0.0.0.0:8001",
        "main": "python central_server/manage.py",
        "start:migrate": "python central_server/manage.py migrate",
        "watch": "tailwindcss -i ./central_server/static/css/config.css -o ./central_server/static/css/styles.css --watch --minify"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "keywords": [
        "districhat",
        "sistributed systems",
        "chat"
    ],
    "bugs": {
        "url": "https://github.com/skye-cyber/districhat/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/skye-cyber/districhat.git"
    },
